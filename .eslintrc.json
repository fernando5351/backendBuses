{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "eqeqeq": ["error", "always"],
    "indent": ["error", "tab"],
    "curly": "error",
    "no-tabs": "off",
		"class-methods-use-this": "off"
  }
}
